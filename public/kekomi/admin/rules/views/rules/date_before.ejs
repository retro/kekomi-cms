<div class="rule-wrap form-inline clearfix date-before-rule" data-rule="date_before">
	<div class="reorder-handle"><i class="icon-sort"></i></div>
	<div class="error"><i class="icon-warning-sign"></i></div>
	<select class="date-fields">
		<option value="">Select Field</option>
		<% for(var field in fields){ %>
			<% if(fields.hasOwnProperty(field) && fields[field] === "date"){ %>
				<option value="<%= field %>" <%= field === selectedField ? "selected" : "" %>><%= field.titleize() %></option>
			<% } %>
		<% } %>
	</select>
	<label>
		<span>has a date before</span>
		<input type="text" class="input-small" <%= (el)-> el.datepicker(); el.datepicker('setDate', (value ? new Date(value) : "+0")) %> />
	</label>
	<a href="javascript://" class="btn btn-small remove-rule"><i class="icon-minus"></i></a>
</div>