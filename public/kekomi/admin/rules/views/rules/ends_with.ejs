<div class="rule-wrap form-inline clearfix ends-with-rule" data-rule="ends_width">
	<div class="error"><i class="icon-warning-sign"></i></div>
	<select class="string-fields">
		<option value="">Select Field</option>
		<% for(var field in fields){ %>
			<% if(fields.hasOwnProperty(field) && fields[field] === "string"){ %>
				<option value="<%= field %>"><%= field %></option>
			<% } %>
		<% } %>
	</select>
	<label>
		<span>ends with</span>
		<input type="text" class="input-small" />
	</label>
	<a href="javascript://" class="btn btn-small remove-rule"><i class="icon-minus"></i></a>
</div>